<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.OnlineWallpaperEditorAdmin" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/color_primary</item>
        <item name="colorPrimaryVariant">@color/color_primary_dark</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/color_secondary</item>
        <item name="colorSecondaryVariant">@color/color_secondary_variant</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/color_primary_dark</item>
        <item name="android:navigationBarColor">@color/white</item>
        <!-- Customize your theme here. -->
        <item name="fontFamily">@font/font_family</item>
        <item name="android:fontFamily">@font/font_family</item>
    </style>


    <style name="Theme.Paddi.LightDarkTexts" parent="Widget.MaterialComponents.TextView">
        <item name="android:textColor">@color/black_shade</item>
    </style>

    <style name="Divider" parent="Widget.MaterialComponents.TextView">
        <item name="android:background">?android:attr/listDivider</item>
    </style>
</resources>